{
  "manifest_version": 2,
  "name": "fried-rice",
  "version": "0.0.1",
  "description": "快速简易切换 host",
  "default_locale": "zh_CN",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "background": {
    "page": "html/background.html"
  },
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_title": "炒饭好吃",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "proxy",
    "storage"
  ]
}